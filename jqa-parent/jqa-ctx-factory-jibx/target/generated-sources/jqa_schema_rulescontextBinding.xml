<binding xmlns:jqa_ctx="http://www.fluxit.com.ar/jqa/schema/rulescontext" xmlns:jqa="http://www.fluxit.com.ar/jqa/schema/ruleset" trim-whitespace="true">
  <namespace uri="http://www.fluxit.com.ar/jqa/schema/rulescontext" prefix="jqa_ctx"/>
  <mapping abstract="true" type-name="jqa_ctx:RulesContext" class="ar.com.fluxit.jqa.schema.RulesContext">
    <collection get-method="getRulesContextImportList" set-method="setRulesContextImportList" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="jqa_ctx:RulesContextImport" name="rulesContextImport"/>
    </collection>
    <collection get-method="getRuleSetImportList" set-method="setRuleSetImportList" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="jqa_ctx:RuleSetImport" name="ruleSetImport"/>
    </collection>
    <collection get-method="getGlobalPredicateList" set-method="setGlobalPredicateList" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="jqa:Predicate" name="globalPredicate"/>
    </collection>
    <collection get-method="getRuleSetList" set-method="setRuleSetList" usage="optional" create-type="java.util.ArrayList">
      <structure map-as="jqa:Ruleset" name="ruleSet"/>
    </collection>
    <value style="attribute" name="name" get-method="getName" set-method="setName" usage="optional"/>
  </mapping>
  <mapping class="ar.com.fluxit.jqa.schema.RulesContext" name="RulesContext" ns="http://www.fluxit.com.ar/jqa/schema/rulescontext">
    <structure map-as="jqa_ctx:RulesContext"/>
  </mapping>
  <mapping abstract="true" type-name="jqa_ctx:RuleSetImport" class="ar.com.fluxit.jqa.schema.RuleSetImport">
    <value style="attribute" name="name" get-method="getName" set-method="setName" usage="optional"/>
    <value style="attribute" name="fileName" get-method="getFileName" set-method="setFileName" usage="optional"/>
  </mapping>
  <mapping class="ar.com.fluxit.jqa.schema.RuleSetImport" name="RuleSetImport" ns="http://www.fluxit.com.ar/jqa/schema/rulescontext">
    <structure map-as="jqa_ctx:RuleSetImport"/>
  </mapping>
  <mapping abstract="true" type-name="jqa_ctx:RulesContextImport" class="ar.com.fluxit.jqa.schema.RulesContextImport">
    <value style="attribute" name="name" get-method="getName" set-method="setName" usage="optional"/>
    <value style="attribute" name="fileName" get-method="getFileName" set-method="setFileName" usage="optional"/>
  </mapping>
  <mapping class="ar.com.fluxit.jqa.schema.RulesContextImport" name="RulesContextImport" ns="http://www.fluxit.com.ar/jqa/schema/rulescontext">
    <structure map-as="jqa_ctx:RulesContextImport"/>
  </mapping>
</binding>