<?xml version="1.0" encoding="UTF-8"?>
<jqa:Ruleset name="Architecture Ruleset" xmlns:jqa="http://www.fluxit.com.ar/schema/ruleset"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.fluxit.com.ar/jqa/schema/ruleset ../../main/resources/ruleset.xsd ">
	
	<rule name="Service Naming">
		<filterPredicate xsi:type="jqa:AndPredicate">
			<predicate xsi:type="jqa:TypingPredicate" parentClassName="ar.com.osde.framework.Service" />
			<predicate xsi:type="jqa:AbstractionPredicate"
				abstractionType="INTERFACE" />
		</filterPredicate>
		<checkPredicate xsi:type="jqa:NamingPredicate"
			classNamePattern="**Service" />
	</rule> 
	
</jqa:Ruleset>