<jqa:Ruleset name="RulesetTestByFileName"
	xmlns:jqa="http://www.fluxit.com.ar/jqa/schema/ruleset" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.fluxit.com.ar/jqa/schema/ruleset ../../main/resources/schema/ruleset.xsd">

	<rule name="RuleTest" message="RuleTest">
		<filterPredicate xsi:type="jqa:TruePredicate" />
		<checkPredicate xsi:type="jqa:NamingPredicate" namePattern="**" />
	</rule>

	<rule name="PriorityRule" message="PriorityRule" priority="3">
		<filterPredicate xsi:type="jqa:TruePredicate" />
		<checkPredicate xsi:type="jqa:NamingPredicate" namePattern="**" />
	</rule>
	
</jqa:Ruleset>
