<?xml version="1.0" encoding="UTF-8"?>
<plugin>
	<extension
         point="org.eclipse.ui.commands">
       <command
            id="ar.com.fluxit.jqa.command.JQACommand"
            name="Run JQA">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="ar.com.fluxit.jqa.handler.JQAHandler"
            commandId="ar.com.fluxit.jqa.command.JQACommand">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.menus">
      <menuContribution
            locationURI="menu:org.eclipse.ui.run?after=additions">
            <command
                  commandId="ar.com.fluxit.jqa.command.JQACommand"
                  id="ar.com.fluxit.jqa.menu.jdt.JQACommand"
                  mnemonic="J">
            </command>
      </menuContribution>
   </extension>
   <extension
      id="ar.com.fluxit.jqa.JQAMarker"
      name="JQA Problem"
      point="org.eclipse.core.resources.markers">
	   <super
	         type="org.eclipse.core.resources.problemmarker">
	   </super>
	   <persistent
	         value="true">
	   </persistent>
	</extension>
</plugin>
